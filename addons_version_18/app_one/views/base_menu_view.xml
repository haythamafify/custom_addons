<odoo>
    <!-- Root Menu -->
    <menuitem id="menu_property_root"
              name="Properties"
              sequence="10"
              web_icon="app_one,static/description/icon.png"
    />

    <!-- Sub Menu -->
    <menuitem id="menu_property_list"
              name="Properties"
              parent="menu_property_root"
              action="action_property"
              sequence="1"/>

    <menuitem id="configuration_menu"
              name="Configuration"
              parent="menu_property_root"
              groups="app_one.property_manager_group"
              sequence="10"/>
    <!-- Sub Menu -->
    <menuitem id="menu_owner_list"
              name="owners"
              parent="configuration_menu"
              action="action_owner"
              sequence="1"/>

    <!-- Sub Menu -->
    <menuitem id="menu_tag_list"
              name="tags"
              parent="configuration_menu"
              action="action_tag"
              sequence="1"/>


    <!-- Sub Menu -->
    <menuitem id="menu_building_list"
              name="building"
              parent="menu_property_list"
              action="action_building"
              sequence="1"/>


    <menuitem id="menu_property_history"
              name="History"
              parent="menu_property_list"
              action="action_property_history"
              groups="app_one.property_manager_group"

              sequence="2"/>


    <menuitem id="menu_property_owl"
              name="properties owl"
              parent="menu_property_list"
              action="action_property_list_view"
              groups="app_one.property_manager_group"

              sequence="3"/>

    <menuitem id="menu_property_form_owl"
              name="New Property (OWL)"
              parent="menu_property_list"
              action="action_property_form_owl"
              sequence="4"/>


</odoo>